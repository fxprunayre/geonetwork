{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gapi": {
      "projectType": "library",
      "root": "projects/gapi",
      "sourceRoot": "projects/gapi/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/gapi/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/gapi/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/gapi/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "glib": {
      "projectType": "library",
      "root": "projects/glib",
      "sourceRoot": "projects/glib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/glib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/glib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/glib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/glib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "watch": true,
            "headless": false,
            "configFile": "projects/glib/cypress.config.ts"
          }
        },
        "ct": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "glib:serve",
            "watch": true,
            "headless": false,
            "testingType": "component"
          },
          "configurations": {
            "development": {
              "devServerTarget": "glib:serve:development"
            }
          }
        }
      }
    },
    "gapp": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/gapp",
      "sourceRoot": "projects/gapp/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/gapp",
            "index": "projects/gapp/src/index.html",
            "browser": "projects/gapp/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/gapp/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/gapp/public"
              }
            ],
            "styles": [
              "projects/gapp/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1.5MB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/gapp/src/environments/environment.ts",
                  "with": "projects/gapp/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "gapp:build:production"
            },
            "development": {
              "buildTarget": "gapp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/gapp/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/gapp/public"
              }
            ],
            "styles": [
              "projects/gapp/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "gapp:serve",
            "configFile": "projects/gapp/cypress.config.ts"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gapp:serve:production"
            }
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "watch": true,
            "headless": false,
            "configFile": "projects/gapp/cypress.config.ts"
          }
        },
        "ct": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "gapp:serve",
            "watch": true,
            "headless": false,
            "testingType": "component"
          },
          "configurations": {
            "development": {
              "devServerTarget": "gapp:serve:development"
            }
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "gapp:serve",
            "watch": true,
            "headless": false,
            "configFile": "projects/gapp/cypress.config.ts"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gapp:serve:production"
            }
          }
        }
      }
    },
    "gwebcomponents": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/gwebcomponents",
      "sourceRoot": "projects/gwebcomponents/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/gwebcomponents",
            "index": "projects/gwebcomponents/src/index.html",
            "browser": "projects/gwebcomponents/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/gwebcomponents/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/gwebcomponents/public"
              }
            ],
            "styles": [
              "projects/gwebcomponents/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "400kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "gwebcomponents:build:production"
            },
            "development": {
              "buildTarget": "gwebcomponents:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/gwebcomponents/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/gwebcomponents/public"
              }
            ],
            "styles": [
              "projects/gwebcomponents/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@cypress/schematic",
      "@schematics/angular"
    ],
    "analytics": false
  }
}
