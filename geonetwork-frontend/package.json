{
  "name": "geonetwork",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build-all": "ng build gapi && ng build glib && ng build gapp && ng build gwebcomponents --output-hashing none",
    "build-lib-gapi": "ng build gapi",
    "build-lib-glib": "ng build glib",
    "build-lib-gwebcomponents": "ng build gwebcomponents --output-hashing none",
    "build-gapp": "ng build gapp",
    "start": "ng serve --project=gapp",
    "start-webcomponents": "ng serve --project=gwebcomponents",
    "watch-lib": "ng build gapi --watch && ng build glib --watch",
    "prettier:check": "prettier --check \"**\"",
    "prettier": "prettier --write \"**\"",
    "test": "ng test glib",
    "openapi-generator": "openapi-generator-cli generate -i projects/gapi/gapi.json  -g typescript-fetch -o  projects/gapi/src/lib  --skip-validate-spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.6",
    "@angular/common": "^18.0.6",
    "@angular/compiler": "^18.0.6",
    "@angular/core": "^18.0.6",
    "@angular/elements": "^18.0.6",
    "@angular/forms": "^18.0.6",
    "@angular/platform-browser": "^18.0.6",
    "@angular/platform-browser-dynamic": "^18.0.6",
    "@angular/router": "^18.0.6",
    "@elastic/elasticsearch": "^8.14.0",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@ngrx/operators": "^18.0.0-rc.1",
    "@ngrx/signals": "^18.0.0-rc.2",
    "papaparse": "^5.4.1",
    "primeng": "^17.18.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.0.6",
    "@cypress/schematic": "^2.5.2",
    "@eslint/js": "^9.6.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/jasmine": "~5.1.0",
    "@types/papaparse": "^5.3.14",
    "angular-eslint": "18.1.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.13.1",
    "globals": "^15.8.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.0.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.4.2"
  }
}
